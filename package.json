{
  "name": "@edx/create-edx-react-app",
  "version": "0.0.0-development",
  "description": "A CLI to generate a React application for edX.",
  "main": "build/index.js",
  "bin": {
    "create-edx-react-app": "build/index.js"
  },
  "scripts": {
    "build": "babel src/ -d build/ --copy-files --include-dotfiles --delete-dir-on-start",
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "deploy": "npm run travis-deploy-once 'npm run semantic-release'",
    "lint": "eslint --ext .js .",
    "test": "jest src/ --coverage --passWithNoTests",
    "prepare": "npm run build",
    "semantic-commit": "commit",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edx/create-edx-react-app.git"
  },
  "keywords": [
    "edx",
    "frontend",
    "cli"
  ],
  "license": "AGPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/edx/create-edx-react-app/issues"
  },
  "homepage": "https://github.com/edx/create-edx-react-app#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-transform-async-to-generator": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "@commitlint/prompt": "^6.1.3",
    "@commitlint/prompt-cli": "^6.1.3",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "codecov": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "semantic-release": "^15.1.8",
    "travis-deploy-once": "^5.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.46",
    "axios": "^0.18.0",
    "chalk": "^2.4.1",
    "child-process-promise": "^2.2.1",
    "commander": "^2.15.1",
    "extract-zip": "^1.6.7",
    "fs-extra": "^5.0.0",
    "inquirer": "^5.2.0",
    "is-json": "^2.0.1",
    "is-online": "^7.0.0",
    "js-yaml": "^3.11.0",
    "move-file": "^1.0.0",
    "normalize-package-data": "^2.4.0",
    "sort-package-json": "^1.13.0",
    "untildify": "^3.0.2",
    "validate-npm-package-name": "^3.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  }
}
